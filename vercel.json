{
    "builds": [{
        "src": "Backend/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    }],
    "routes": [
        {
            "src": "/api/upload",
            "methods": ["POST"],
            "dest": "Backend/wsgi.py"
        },
        {
            "src": "/(.*)",
            "dest": "Backend/wsgi.py"
        }
    ]
}
